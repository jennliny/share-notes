<div class="container">
  <div class="col-sm-6">
      <img src="<%= user.imageURL %>" alt="image of user">
      <img src="uploads/<%= user.imageFileName %>" alt="image of user">
  </div>
  <div class="col-sm-6">
      <table class="table table-bordered">
        <tbody>
          <tr>
            <td>Name</td>
            <td><%= user.googlename %> </td>
          </tr>
          <tr>
            <td>Email</td>
            <td><%= user.googleemail %> </td>
          </tr>
          <tr>
            <td>Username</td>
            <td><%= user.username %> </td>
          </tr>
        </tbody>
        <tfoot>
          <tr>
          <td>&nbsp;</td>
          <td><a href="/editProfile">Edit Profile</a></td>
          </tr>
          </tfoot>
      </table>
      <ul>
        <li><a href="/showFilteredNotes">See your notes</a></li>
      </ul>
  </div>
</div>

